{
  "packages": {
    "packages/tiny-png": {
      "component": "tiny-png",
      "release-type": "node"
    },
    "packages/tiny-qr": {
      "component": "tiny-qr",
      "release-type": "node"
    },
    "packages/tiny-qr-png": {
      "component": "tiny-qr-png",
      "release-type": "node"
    },
    "packages/tiny-qr-svg": {
      "component": "tiny-qr-svg",
      "release-type": "node"
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-type": "node",
  "bump-minor-pre-major": false,
  "bump-patch-for-minor-pre-major": false,
  "draft": false,
  "prerelease": false,
  "changelog-path": "CHANGELOG.md",
  "always-link-local": false,
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false,
      "updateAllPackages": true,
      "updatePeerDependencies": true
    }
  ]
}
